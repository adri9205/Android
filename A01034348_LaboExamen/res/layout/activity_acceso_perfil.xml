<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="${relativePackage}.${activityClass}" >
        <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bienvenido"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/nombretv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/mailtv"
        android:layout_alignParentLeft="true"
        android:text="Nombre"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/paistv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/webView1"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="29dp"
        android:text="PaÃ­s"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/salirb"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView1"
        android:text="Salir" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/salirb"
        android:layout_marginLeft="21dp"
        android:layout_toRightOf="@+id/textView1"
        android:hint="&lt;username>"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/mailtv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/paistv"
        android:layout_alignParentLeft="true"
        android:text="Mail"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/agregarb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="32dp"
        android:text="Agregar Perfil" />

    <WebView
        android:id="@+id/webView1"
        android:layout_width="match_parent"
        android:layout_height="154dp"
        android:layout_above="@+id/agregarb"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="26dp"
        android:layout_weight="0.63" />

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/webView1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

</RelativeLayout>